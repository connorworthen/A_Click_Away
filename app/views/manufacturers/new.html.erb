<h1>Create New Manufacturer</h1>

<%= form_with(model: @manufacturer, local: true) do |f| %>

<%= f.label :name %>
<%= f.text_field :name %>

<%= f.label :established %>
<%= f.number_field :established, step: 2020 %>

<%= f.label :bio %>
<%= f.text_field :bio %>

<%= f.submit "Create Manufacturer" %>

<% end %>